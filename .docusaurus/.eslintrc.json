{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "airbnb",
      "prettier"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": [
      "react",
      "jsx-a11y",
      "import"
    ],
    "rules": {
      "prettier/prettier": ["error"],
      "react/react-in-jsx-scope": "off"
    }
  }
  